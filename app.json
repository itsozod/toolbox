{
  "expo": {
    "name": "toolbox",
    "slug": "toolbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "toolbox",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ozod2905.toolbox",
      "icon": {
        "light": "./src/shared/assets/images/ios-light.png",
        "dark": "./src/shared/assets/images/ios-dark.png",
        "tinted": "./src/shared/assets/images/ios-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.ozod2905.toolbox"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/shared/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-compressor",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/shared/assets/images/splash-icon-dark.png",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./src/shared/assets/images/splash-icon-light.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "510a141c-27ba-4c47-85e7-e54aa6022427"
      }
    }
  }
}
