{
  "expo": {
    "name": "toolbox",
    "slug": "toolbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": ".src/shared/assets/images/icon.png",
    "scheme": "toolbox",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": ".src/shared/assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ozod2905.toolbox"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ".src/shared/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.ozod2905.toolbox"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ".src/shared/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-compressor",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "510a141c-27ba-4c47-85e7-e54aa6022427"
      }
    }
  }
}
